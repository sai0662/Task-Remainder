{
    "name": "Task Remainder",
    "description": "My First test extension",
    "version": "1.0.0",
    "manifest_version": 3,
    "action": {
        "default_title": "Chrome Ext",
        "default_popup": "popup.html"
         },
    "icons": {"128": "icon.png"},
    "background": {
        "service_worker": "./popup.js"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ]

}